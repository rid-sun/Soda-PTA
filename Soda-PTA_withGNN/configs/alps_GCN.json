{
    "gpu": {
        "use": true,
        "id": 0
    },

    "model": "GCN",
    "dataset": "CIRCUIT_PTA_GCN",

    "params": {
        "epochs": 250,
        "batch_size": 8,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.0001,
        "print_epoch_interval": 5,
        "max_time": 12
    },

    "net_params": {
        "L": 3,
        "hidden_dim": 64,
        "in_dim": 16,
        "out_dim": 3,
        "residual": true,
        "readout": "sum",
        "in_feat_dropout": 0.0,
        "dropout": 0.1,
        "batch_norm": true,
        "self_loop": false,
        "dtype": "float"
    }
}
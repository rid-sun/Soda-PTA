{
    "gpu": {
        "use": true,
        "id": 0
    },

    "model": "GAT",
    "dataset": "CIRCUIT_PTA_GAN",

    "params": {
        "seed": 41,
        "epochs": 250,
        "batch_size": 16,
        "init_lr": 0.001,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 10,
        "min_lr": 1e-5,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },

    "net_params": {
        "L": 4,
        "hidden_dim": 19,
        "out_dim": 152,
        "residual": true,
        "readout": "mean",
        "n_heads": 8,
        "in_feat_dropout": 0.0,
        "dropout": 0.0,
        "batch_norm": true,
        "self_loop": false
    }
}